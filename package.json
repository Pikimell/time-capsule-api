{
  "name": "aws-node-express-template",
  "version": "1.0.0",
  "description": "Aws Node Express Template",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"ts-node --esm\" src/index.ts",
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "push": "git add . && git commit -m 'update' && git push",
    "deploy": "npm run build && sls deploy"
  },
  "keywords": [],
  "author": "Volodymyr Pashchenko",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "amazon-cognito-identity-js": "^6.3.15",
    "aws-jwt-verify": "^4.0.1",
    "aws-sdk": "^2.1692.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "mongoose": "^8.14.1",
    "serverless-http": "^3.2.0",
    "useragent": "^2.3.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.4",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
